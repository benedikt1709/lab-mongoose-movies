{{#each Celeb}}
<p>
    <h2>{{this.name}}</h2>
    <a href="/celebrities/{{this._id}}" class="button">Show Details</a>
    <form action="/celebrities/{{this._id}}/delete" method="post">
    <button type="submit">Delete</button>
    </form>
</p>
{{/each}}
<a href="/celebrities/new" class="button">Add Celebrity</a>